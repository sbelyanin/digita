{
  "service": {
    "name": "nomad-ui",

    "tags": [
          "traefik.enable=true",
          "traefik.frontend.entryPoints=https",
          "traefik.frontend.rule=Host:nomad-ui"
    ],

    "address": "nomad-servers.service.consul",
    "port": 4646,
    "check": [
      {
          "name": "alive",
          "interval": "20s",
          "http": "http://nomad-servers.service.consul:4646",
          "timeout": "2s"
      }
    ]
  }
}

