{
  "service": {
    "name": "consul-ui",

    "tags": [
          "traefik.enable=true",
          "traefik.frontend.entryPoints=http",
          "traefik.frontend.rule=Host:consul-ui"
    ],

    "address": "127.0.0.1",
    "port": 8500,
    "check": [
      {
          "name": "alive",
          "http": "http://127.0.0.1:8500",
          "interval": "20s",
          "timeout": "2s"
      }
    ]
  }
}

