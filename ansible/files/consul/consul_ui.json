{
  "service": {
    "name": "consul-ui",

    "tags": [
          "traefik.enable=true",
          "traefik.frontend.entryPoints=https",
          "traefik.frontend.rule=Host:consul-ui"
    ],

    "address": "consul.service.consul",
    "port": 8500,
    "check": [
      {
          "name": "alive",
          "http": "http://consul.service.consul:8500",
          "interval": "20s",
          "timeout": "2s"
      }
    ]
  }
}

